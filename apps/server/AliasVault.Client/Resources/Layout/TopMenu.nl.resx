<?xml version="1.0" encoding="utf-8"?>
<root>
  <xsd:schema id="root" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xsd:element name="root" msdata:IsDataSet="true">
      <xsd:complexType>
        <xsd:choice maxOccurs="unbounded">
          <xsd:element name="data">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="value" type="xsd:string" minOccurs="0" />
                <xsd:element name="comment" type="xsd:string" minOccurs="0" />
              </xsd:sequence>
              <xsd:attribute name="name" type="xsd:string" />
              <xsd:attribute name="type" type="xsd:string" />
              <xsd:attribute name="mimetype" type="xsd:string" />
            </xsd:complexType>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
    </xsd:element>
  </xsd:schema>

  <!-- Main navigation links -->
  <data name="CredentialsNav">
    <value>Inloggegevens</value>
    <comment>Main navigation link for credentials section</comment>
  </data>
  <data name="EmailsNav">
    <value>E-mails</value>
    <comment>Main navigation link for emails section</comment>
  </data>

  <!-- Settings menu items -->
  <data name="GeneralSettingsNav">
    <value>Algemene instellingen</value>
    <comment>Navigation link for general settings</comment>
  </data>
  <data name="SecuritySettingsNav">
    <value>Beveiligingsinstellingen</value>
    <comment>Navigation link for security settings</comment>
  </data>
  <data name="ImportExportNav">
    <value>Importeren / Exporteren</value>
    <comment>Navigation link for import/export settings</comment>
  </data>
  <data name="ExtensionsAppsNav">
    <value>Extensies &amp; Apps</value>
    <comment>Navigation link for extensions and apps settings</comment>
  </data>

  <!-- Menu actions -->
  <data name="ToggleDarkMode">
    <value>Donkere modus schakelen</value>
    <comment>Button text for toggling dark/light theme</comment>
  </data>
  <data name="LogOut">
    <value>Uitloggen</value>
    <comment>Button text for logging out</comment>
  </data>

  <!-- Accessibility labels -->
  <data name="OpenMenuLabel">
    <value>Menu openen</value>
    <comment>Screen reader label for mobile menu button</comment>
  </data>

  <!-- Status labels -->
  <data name="NewLabel">
    <value>NIEUW</value>
    <comment>Label for new features or menu items</comment>
  </data>
  <data name="BetaLabel">
    <value>BETA</value>
    <comment>Label indicating beta version of the application</comment>
  </data>
</root>